# Environment
DriveLetter<-
  function(){
    return(
      c("E:/","~/")[
        match(
          Sys.info()["nodename"],
          c("DESKTOP-SI6GITG","MacBook-Pro-2.local"))])}
data.loc<-
  function(){
    return(
      paste(
        DriveLetter(),
        "GoogleDrive/ResearchTeam/Datahouse/REducation/",
        sep=""))}

